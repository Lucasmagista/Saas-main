# Exemplo de configuração de ambiente para a plataforma SaaS

# URL e chave do Supabase (obrigatório)
SUPABASE_URL=
SUPABASE_KEY=

# Porta do servidor HTTP (Express)
PORT=3002

# Chaves de integração WPPConnect (por exemplo, token, sessão, etc.)
WPP_SESSION=
WPP_TOKEN=

# Integração com IAs generativas
OPENAI_KEY=
GEMINI_KEY=
CLAUDE_KEY=
COHERE_KEY=
MISTRAL_KEY=
# (adicione outras chaves de IA conforme necessário)