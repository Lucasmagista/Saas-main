{
  "name": "vite_react_shadcn_ts",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && node index.cjs",
    "dev:frontend": "vite",
    "start": "vite",
    "start:frontend": "vite",
    "start:backend": "node backend/index.cjs",
    "start:all": "concurrently \"npm run start:frontend\" \"npm run start:backend\" --names \"frontend,backend\" --prefix-colors \"cyan,yellow\"",
    "build": "vite build",
    "build:dev": "vite build --mode development",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:backend": "jest backend/tests --testEnvironment=node",
    "test:frontend": "jest src --testEnvironment=jsdom",
    "test:integration": "jest --testPathPattern=integration",
    "test:smoke:staging": "npm run test:smoke -- --env=staging",
    "test:smoke:production": "npm run test:smoke -- --env=production",
    "test:smoke": "jest --testPathPattern=smoke",
    "db:migrate": "cd backend && node scripts/migrate.cjs",
    "db:migrate:test": "cd backend && NODE_ENV=test node scripts/migrate.cjs",
    "db:seed": "cd backend && node scripts/seed.cjs",
    "db:reset": "cd backend && node scripts/reset.cjs",
    "docker:build": "docker build -t saas-platform .",
    "docker:run": "docker run -p 3000:3000 -p 3002:3002 saas-platform",
    "docker:compose": "docker-compose up -d",
    "docker:compose:down": "docker-compose down",
    "clean": "rm -rf dist node_modules/.cache coverage",
    "clean:all": "rm -rf dist node_modules coverage .nyc_output",
    "audit": "npm audit",
    "audit:fix": "npm audit fix",
    "security:check": "npm audit --audit-level=moderate"
  },
